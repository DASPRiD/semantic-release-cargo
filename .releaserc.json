{
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    "@semantic-release/changelog",
    [
      "@semantic-release/exec",
      {
        "verifyConditionsCmd": "./target/release/semantic-release-cargo verify-conditions",
        "prepareCmd": "./target/release/semantic-release-cargo prepare ${nextRelease.version}",
        "publishCmd": "./target/release/semantic-release-cargo publish"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": "Cargo.toml"
      }
    ]
  ]
}
